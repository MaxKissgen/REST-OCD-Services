<ivy-module version="2.0">
    <info organisation="i5" module="ocd-service"/>
    <configurations defaultconfmapping="standard->*;derby->*">
        <conf name="standard" visibility="public"></conf>
        <conf name="derby" visibility="public"></conf>
    </configurations>
    <dependencies defaultconf="standard">
        <dependency org="org.apache.commons" name="commons-exec" rev="1.3"/>
        <dependency org="org.apache.derby" name="derbynet" rev="10.11.1.1" conf="derby"/>
        <dependency org="org.apache.derby" name="derbytools" rev="10.11.1.1" conf="derby"/>
        <dependency org="org.apache.derby" name="derbyclient" rev="10.11.1.1" conf="derby"/>
        <dependency org="org.apache.derby" name="derby" rev="10.11.1.1" conf="derby"/>
        <dependency org="com.google.guava" name="guava" rev="18.0"/>
        <dependency org="org.la4j" name="la4j" rev="0.4.9"/>
        <dependency org="org.eclipse.persistence" name="eclipselink" rev="2.5.2"/>
        <dependency org="org.apache.geronimo.specs" name="geronimo-jpa_2.0_spec" rev="1.1"/>
        <dependency org="commons-jxpath" name="commons-jxpath" rev="1.1"/>
        <dependency org="commons-logging" name="commons-logging" rev="1.2"/>
        
        <dependency org="com.j256.ormlite" name="ormlite-core" rev="4.48"/>
    	<dependency org="com.j256.ormlite" name="ormlite-jdbc" rev="4.48"/>
        
    </dependencies>
</ivy-module>
